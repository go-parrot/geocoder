{
    "name": "@goparrot/geocoder",
    "description": "TypeScript GeoCoder, node geocoding library, supports google maps, mapquest, open street map, tom tom",
    "version": "0.0.1",
    "author": "Coroliov Oleg",
    "license": "MIT",
    "private": false,
    "bugs": "https://github.com/go-parrot/geocoder/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/go-parrot/geocoder"
    },
    "engines": {
        "node": ">=8.9.0"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "husky": {
        "hooks": {
            "pre-commit": "npm run pre-commit",
            "post-commit": "git update-index --again"
        }
    },
    "lint-staged": {
        "linters": {
            "*.{ts,json}": [
                "npm run format",
                "git add"
            ]
        }
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "scripts": {
        "prepublishOnly": "npm run build",
        "build": "rimraf dist && tsc",
        "pre-commit": "git add . && npm run format:staged && npm run lint && npm run test && npm run build",
        "format": "prettier \"**/*.{ts,json}\" --write",
        "format:staged": "lint-staged",
        "lint": "npm run lint:config:check && tslint -c tslint.json -p tsconfig.json --format stylish && tslint -c tslint.json -p tsconfig.spec.json --format stylish",
        "lint:config:check": "tslint-config-prettier-check ./tslint.json",
        "test": "nyc mocha 'test/unit/**/*.spec.ts' 'test/e2e/**/*.spec.ts'",
        "test:unit": "nyc mocha 'test/unit/**/*.spec.ts'",
        "test:e2e": "nyc mocha 'test/e2e/**/*.spec.ts'",
        "test:integration": "nyc mocha --timeout 30000 'test/integration/**/*.spec.ts'",
        "test:ci": "nyc mocha --timeout 30000 'test/**/*.spec.ts'",
        "coverage:ci": "nyc report --reporter=text-lcov | coveralls",
        "commit": "git-cz",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "preversion": "npm run pre-commit",
        "version": "npm run changelog",
        "postversion": "git push && git push --tags"
    },
    "dependencies": {
        "class-transformer": "0.2.0",
        "class-validator": "0.9.1",
        "world-countries": "2.1.0"
    },
    "devDependencies": {
        "@types/chai": "4.1.7",
        "@types/chai-as-promised": "7.1.0",
        "@types/mocha": "5.2.6",
        "@types/node": "11.9.5",
        "@types/sinon": "7.0.6",
        "@types/supertest": "2.0.7",
        "axios": "0.19.0-beta.1",
        "axios-mock-adapter": "1.16.0",
        "chai": "4.2.0",
        "chai-as-promised": "7.1.1",
        "commitizen": "3.0.7",
        "conventional-changelog-cli": "2.0.12",
        "coveralls": "3.0.3",
        "cz-conventional-changelog": "2.1.0",
        "dotenv": "6.2.0",
        "husky": "1.3.1",
        "lint-staged": "8.1.4",
        "mocha": "6.0.2",
        "mocha-junit-reporter": "1.18.0",
        "nyc": "13.3.0",
        "prettier": "1.16.4",
        "reflect-metadata": "0.1.13",
        "rimraf": "2.6.3",
        "sinon": "7.2.4",
        "source-map-support": "0.5.10",
        "supertest": "3.4.2",
        "ts-node": "8.0.2",
        "tsconfig-paths": "3.8.0",
        "tslint": "5.13.0",
        "tslint-config-prettier": "1.18.0",
        "typescript": "3.3.3333",
        "winston": "3.2.1"
    },
    "peerDependencies": {
        "axios": "^0.19.0-beta.1",
        "reflect-metadata": "0.1.13"
    }
}
